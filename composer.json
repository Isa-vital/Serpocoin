{
    "name": "serpocoin/website",
    "description": "Serpo Coin - A cosmic legend reborn in Web3",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "mailjet/mailjet-apiv3-php": "^1.6",
        "vlucas/phpdotenv": "^5.6"
    },
    "autoload": {
        "psr-4": {
            "Serpocoin\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
